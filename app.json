{
  "addons": [
    "jawsdb"
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "JAWSDB_DATABASE": {
      "required": true
    },
    "JAWSDB_HOST": {
      "required": true
    },
    "JAWSDB_PASSWORD": {
      "required": true
    },
    "JAWSDB_URL": {
      "required": true
    },
    "JAWSDB_USERNAME": {
      "required": true
    },
    "TOKEN_SECRET": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "travel-map-api",
  "scripts": {
  },
  "stack": "heroku-18"
}
